{
  "name": "@fepack/image",
  "version": "0.2.1",
  "description": "Image preloader for fepack",
  "keywords": [
    "fepack",
    "image",
    "preloader"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fepack/image.git",
    "directory": "packages/image"
  },
  "license": "MIT",
  "author": "tooooo1",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc && pnpm build:js && pnpm copy:cts",
    "build:js": "node ./esbuild.config.js",
    "build:tsc": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "copy:cts": "cp dist/index.d.ts dist/index.d.cts",
    "lint": "eslint \"**/*.ts*\"",
    "lint:pub": "publint --strict",
    "prepack": "pnpm build",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@fepack/eslint-config-js": "workspace:*",
    "@fepack/eslint-config-ts": "workspace:*",
    "@fepack/tsconfig": "workspace:*",
    "@types/node": "^20.6.2",
    "@vitest/browser": "^0.34.4",
    "@vitest/coverage-istanbul": "^0.34.4",
    "esbuild": "^0.18.11",
    "jsdom": "^22.1.0",
    "playwright": "^1.38.0",
    "vitest": "^0.34.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
