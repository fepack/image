{
  "name": "@frontend-box/image-preloader",
  "version": "0.0.0",
  "description": "Image preloader for Frontend-Box",
  "keywords": [
    "frontend-box",
    "image",
    "preloader"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tooooo1/frontend-box.git",
    "directory": "packages/image-preloader"
  },
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc && pnpm build:js",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:js": "node ./esbuild.config.js",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@frontend-box/tsconfig": "workspace:*",
    "esbuild": "^0.18.11",
    "typescript": "^4.9.4"
  }
}
