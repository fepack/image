{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "lint:pub": { "cache": false },
    "prepack": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "type:check": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
